   "source": [
    "import pandas as pd\n",
    "import geopandas as gpd\n",
    "from shapely.geometry import Point\n",
    "from geopy.distance import geodesic\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "# Load the datasets\n",
    "facilities_df = pd.read_csv('your_local_path/England_merged_map_features2.csv')\n",
    "litter_df = pd.read_csv('your_local_path/polite_env/England_part1.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "# Convert lat/lon to geometry points for both datasets\n",
    "facilities_gdf = gpd.GeoDataFrame(\n",
    "    facilities_df,\n",
    "    geometry=gpd.points_from_xy(facilities_df.lon, facilities_df.lat),\n",
    "    crs=\"EPSG:4326\"\n",
    ")\n",
    "\n",
    "litter_gdf = gpd.GeoDataFrame(\n",
    "    litter_df,\n",
    "    geometry=gpd.points_from_xy(litter_df.center_long, litter_df.center_lat),\n",
    "    crs=\"EPSG:4326\"\n",
    ")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert to projected CRS (meters) for accurate distance measurement\n",
    "facilities_gdf = facilities_gdf.to_crs(epsg=32618)  # UTM Zone 17N\n",
    "litter_gdf = litter_gdf.to_crs(epsg=32618)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "## correct\n",
    "# Initialize list for storing final results\n",
    "litter_results = []\n",
    "\n",
    "# Iterate through each litter point\n",
    "for _, litter in litter_gdf.iterrows():\n",
    "    # Store litter point details\n",
    "    litter_result = {\n",
    "        'litterId': litter['litterId'],\n",
    "        'lat': litter['lat'],\n",
    "        'long': litter['long'],\n",
    "        'timestamp': litter['timestamp'],\n",
    "        'city': litter['city'],\n",
    "        'country': litter['country'],\n",
    "        'state' : litter['state'],\n",
    "        'userTags': litter['userTags'],\n",
    "        'material_category': litter['category'],\n",
    "        'material_category_by_location': litter['category_by_location'],\n",
    "        'category_center_lat': litter['center_lat'],\n",
    "        'category_center_long': litter['center_long']\n",
    "    }\n",
    "    \n",
    "    # Calculate distances between the current litter point and all facilities\n",
    "    facilities_with_distance = facilities_gdf.copy()\n",
    "    facilities_with_distance['distance'] = facilities_with_distance.distance(litter.geometry)\n",
    "    \n",
    "    # Filter facilities within 500 meters\n",
    "    nearby_facilities = facilities_with_distance[facilities_with_distance['distance'] <= 500]\n",
    "    \n",
    "    if not nearby_facilities.empty:\n",
    "        # Count amenities by category\n",
    "        category_counts = nearby_facilities['map_feature_category'].value_counts().to_dict()\n",
    "        \n",
    "        if category_counts:  # Check if category_counts is not empty\n",
    "            # Total number of amenities\n",
    "            total_amenities = sum(category_counts.values())\n",
    "            litter_result['total_amenities'] = total_amenities\n",
    "\n",
    "            # Find the largest category count\n",
    "            largest_count = max(category_counts.values())\n",
    "\n",
    "            # Get categories with the largest count\n",
    "            largest_categories = [\n",
    "                category for category, count in category_counts.items() if count == largest_count\n",
    "            ]\n",
    "            \n",
    "            # If multiple categories are tied, use material_category to prioritize\n",
    "            selected_category = None\n",
    "            if len(largest_categories) > 1:\n",
    "                # Define mapping of material categories to amenity categories\n",
    "                material_to_amenity = {\n",
    "                    \"Plastic Litter\": [\"Utilities and Infrastructure\", \"Recreation and Outdoors\", \"Food and Beverage\", \"wetland\",\"water\"],\n",
    "                    \"Metal Litter\": [\"Utilities and Infrastructure\", \"Transportation\"],\n",
    "                    \"Glass Litter\": [\"Food and Beverage\", \"Recreation and Outdoors\"],\n",
    "                    \"Paper and Cardboard Litter\": [\"Education\", \"Food and Beverage\", \"Public Services\"],\n",
    "                    \"Organic Litter\": [\"Recreation and Outdoors\", \"Food and Beverage\"],\n",
    "                    \"Cigarette Litter\": [\"Transportation\", \"Recreation and Outdoors\"],\n",
    "                    \"E-Waste (Electronic Waste)\": [\"Utilities and Infrastructure\", \"Public Services\"],\n",
    "                    \"Hazardous Waste\": [\"Health and Emergency Services\", \"Utilities and Infrastructure\"],\n",
    "                    \"Rubber and Tire Litter\": [\"Transportation\", \"Recreation and Outdoors\"],\n",
    "                    \"Marine Litter\": [\"Recreation and Outdoors\"],\n",
    "                    \"Cloth Litter\": [\"Accommodation and Living\", \"Recreation and Outdoors\"]\n",
    "                }\n",
    "                \n",
    "                # Get the material category of the litter\n",
    "                material_category = litter['category']\n",
    "                prioritized_amenities = material_to_amenity.get(material_category, [])\n",
    "                \n",
    "                # Select the first matching category in the prioritized list\n",
    "                for amenity in prioritized_amenities:\n",
    "                    if amenity in largest_categories:\n",
    "                        selected_category = amenity\n",
    "                        break\n",
    "                \n",
    "                # If no match found, just pick the first largest category\n",
    "                if not selected_category:\n",
    "                    selected_category = largest_categories[0]\n",
    "            else:\n",
    "                # If only one largest category, select it\n",
    "                selected_category = largest_categories[0]\n",
    "            \n",
    "            # Add details of the selected category\n",
    "            selected_facility = nearby_facilities[nearby_facilities['map_feature_category'] == selected_category].iloc[0]\n",
    "            \n",
    "            litter_result['amenity_title'] = selected_facility['map_feature_title']\n",
    "            litter_result['amenity_category'] = selected_category\n",
    "            litter_result['selected_amenities_counts'] = str(category_counts[selected_category])\n",
    "            litter_result['amenity_lat'] = str(selected_facility['lat'])\n",
    "            litter_result['amenity_lon'] = str(selected_facility['lon'])\n",
    "        else:\n",
    "            # If category_counts is empty, set related fields to None\n",
    "            litter_result['total_amenities'] = 0\n",
    "            litter_result['amenity_title'] = None\n",
    "            litter_result['amenity_category'] = None\n",
    "            litter_result['selected_amenities_counts'] = None\n",
    "            litter_result['amenity_lat'] = None\n",
    "            litter_result['amenity_lon'] = None\n",
    "    else:\n",
    "        # If no nearby amenities, set all related fields to None\n",
    "        litter_result['total_amenities'] = 0\n",
    "        litter_result['amenity_title'] = None\n",
    "        litter_result['amenity_category'] = None\n",
    "        litter_result['selected_amenities_counts'] = None\n",
    "        litter_result['amenity_lat'] = None\n",
    "        litter_result['amenity_lon'] = None\n",
    "\n",
    "    # Append results for this litter point\n",
    "    litter_results.append(litter_result)\n",
    "    \n",
    "\n",
    "\n",
    "  # Initialize list for storing final results with nearest amenity if total_amenities is 0\n",
    "updated_litter_results = []\n",
    "\n",
    "for litter_result in litter_results:\n",
    "    # Check if total_amenities is 0\n",
    "    if litter_result['total_amenities'] == 0:\n",
    "        # Find the corresponding litter point in the GeoDataFrame\n",
    "        litter = litter_gdf[litter_gdf['litterId'] == litter_result['litterId']].iloc[0]\n",
    "\n",
    "        # Calculate distances between the current litter point and all facilities\n",
    "        facilities_with_distance = facilities_gdf.copy()\n",
    "        facilities_with_distance['distance'] = facilities_with_distance.distance(litter.geometry)\n",
    "\n",
    "        # Find the nearest facility\n",
    "        nearest_facility = facilities_with_distance.nsmallest(1, 'distance').iloc[0]\n",
    "\n",
    "        # Update the litter result with nearest amenity details\n",
    "        litter_result['total_amenities'] = 1  # Since we are assigning a nearest amenity\n",
    "        litter_result['amenity_title'] = nearest_facility['map_feature_title']\n",
    "        litter_result['amenity_category'] = nearest_facility['map_feature_category']\n",
    "        litter_result['selected_amenities_counts'] = 1  # Nearest amenity count is effectively 1\n",
    "        litter_result['amenity_lat'] = nearest_facility['lat']\n",
    "        litter_result['amenity_lon'] = nearest_facility['lon']\n",
    "\n",
    "    # Append the updated litter result\n",
    "    updated_litter_results.append(litter_result)\n",
    "  "
   ]
  },
